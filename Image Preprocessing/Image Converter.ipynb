    "from pathlib import Path\n",
    "from PIL import Image\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [],
   "source": [
    "inputPath = Path(\"./Normal\")\n",
    "inputFiles = inputPath.glob(\"**/*.jpg\")\n",
    "outputPath = Path(\"./Norm-png\")\n",
    "for f in inputFiles:\n",
    "    outputFile = outputPath / Path(f.stem + \".png\")\n",
    "    im = Image.open(f)\n",
    "    im.save(outputFile)"
   ]
  }
